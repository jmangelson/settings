<?xml version="1.0" encoding="UTF-8"?>

<openbox_menu xmlns="http://openbox.org/"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://openbox.org/
                file:///usr/share/openbox/menu.xsd">

<menu id="root-menu" label="Openbox">
  <item label="Terminal">
    <action name="Execute"><execute>myterm</execute></action>
  </item>
  <item label="Web Browser">
    <action name="Execute"><execute>firefox</execute></action>
  </item>
  <!-- This requires the presence of the 'menu' package to work -->
<!--   <menu id="Debian" /> -->
  <separator />
  <menu id="my_apps" label="My Applications">
    <item label="Firefox">
      <action name="Execute"><execute>firefox</execute></action>
    </item>
    <item label="Google Chrome">
      <action name="Execute"><execute>google-chrome</execute></action>
    </item>
    <item label="Emacs">
      <action name="Execute"><execute>myemacs</execute></action>
    </item>
    <item label="Rhythmbox">
      <action name="Execute"><execute>rhythmbox</execute></action>
    </item>
    <item label="Nautilus">
      <action name="Execute"><execute>nautilus --no-desktop</execute></action>
    </item>
    <menu id="my_games" label="Games">
      <item label="Neverball">
	<action name="Execute"><execute>neverball</execute></action>
      </item>
      <item label="SNES9x-Gtk">
	<action name="Execute"><execute>snes9x-gtk</execute></action>
      </item>
      <item label="NES Emulator">
	<action name="Execute"><execute>nesgui-gtk.py</execute></action>
      </item>
    </menu>
  </menu>
  <menu id="client-list-menu" />
  <separator />
  <item label="ObConf">
    <action name="Execute"><execute>obconf</execute></action>
  </item>
  <item label="Reconfigure">
    <action name="Reconfigure" />
  </item>
  <item label="Restart">
    <action name="Restart" />
  </item>
  <separator />
  <item label="Exit">
    <action name="Exit" />
  </item>
</menu>

</openbox_menu>
